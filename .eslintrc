{
    "root": true,
    "env": {
        "es6": true
    },
    "rules": {
        "comma-dangle": [2, "always-multiline"],
        "eol-last": 2,
        "eqeqeq": 2,
        "key-spacing": [2, {"afterColon": true}],
        "keyword-spacing": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-semi": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-spaced-func": 2,
        "no-trailing-spaces": 2,
        "no-undef": 2,
        "no-underscore-dangle": 0,
        "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
        "quotes": [2, "single", "avoid-escape"],
        "semi": [2, "always"],
        "space-before-blocks": 2,
        "space-infix-ops": 2,
        "space-in-parens": 2,
        "strict": [2, "global"]
    }
}
